<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.Group?>
<?import javafx.scene.layout.Pane?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.insa.lifraison.view.MapController">
    <ScrollPane fx:id="scrollPane" hbarPolicy="NEVER" pannable="true" vbarPolicy="NEVER">
        <Group>
            <Pane fx:id="pane" onMouseClicked="#mapClick" prefWidth="1280" prefHeight="500" />
        </Group>
    </ScrollPane>
</VBox>
